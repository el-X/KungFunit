{
  "name": "kungfunit-server",
  "description": "The REST-API and server-side implementation of KungFunit",
  "version": "0.0.1",
  "private": true,
  "author": "Alexander Keller",
  "apidoc": {
    "name": "KungFunit",
    "title": "KungFunit",
    "url": "https://kungfunit.appspot.com",
    "template": {
      "forceLanguage": "en",
      "withCompare": false,
      "withGenerator": false
    }
  },
  "engines": {
    "node": "~4.2"
  },
  "scripts": {
    "start": "node ./bin/www",
    "monitor": "nodemon ./bin/www",
    "deploy": "gcloud preview app deploy --project kungfunit"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "gcloud": "^0.36.0",
    "lodash": "^4.13.1",
    "mathjs": "^3.2.1",
    "moment": "^2.13.0",
    "money": "^0.2.0",
    "morgan": "^1.7.0",
    "request": "^2.72.0",
    "serve-favicon": "~2.3.0",
    "winston": "^2.2.0"
  }
}
